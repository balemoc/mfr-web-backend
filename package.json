{
  "name": "mfr-web-backend",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://mbael@github.com/mbael/mfr-web-backend.git"
  },
  "author": "Balazs Mocsai <me@bael.me>",
  "license": "ISC",
  "scripts": {
    "dev": "webpack --watch & nodemon --watch dist dist/app.js",
    "prod": "webpack",
    "doc": "docco src/**/*.js "
  },
  "bugs": {
    "url": "https://mbael@github.com/mbael/mfr-web-backend/issues"
  },
  "homepage": "https://mbael@github.com/mbael/mfr-web-backend#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-node5": "^11.1.0",
    "babel-root-import": "^4.1.0",
    "code": "^3.0.2",
    "docco": "^0.7.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-lab": "^1.0.6",
    "gulp-nodemon": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "hapi-cors-headers": "^1.0.0",
    "lab": "^11.0.1",
    "pump": "^1.0.1",
    "uglify-js": "^2.7.3",
    "uglify-js-harmony": "^2.6.2",
    "webpack": "^1.13.2"
  },
  "babel": {
    "presets": [
      "node5"
    ],
    "plugins": [
      [
        "babel-root-import",
        [
          {
            "rootPathPrefix": "~",
            "rootPathSuffix": "src/"
          }
        ]
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^[~]"
          ]
        }
      ],
      "babel/generator-star-spacing": 1,
      "babel/new-cap": 1,
      "babel/array-bracket-spacing": 1,
      "babel/object-curly-spacing": 1,
      "babel/object-shorthand": 1,
      "babel/arrow-parens": 1,
      "babel/no-await-in-loop": 1,
      "babel/flow-object-type": 1,
      "indent": [
        "error",
        2
      ]
    },
    "plugins": [
      "babel"
    ],
    "extends": [
      "airbnb"
    ]
  },
  "dependencies": {
    "async": "^2.0.1",
    "bcrypt": "^0.8.7",
    "blipp": "^2.3.0",
    "bluebird": "^3.4.6",
    "chance": "^1.0.4",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-file": "^6.0.1",
    "good-squeeze": "^4.0.0",
    "hapi": "^15.0.3",
    "hapi-auth-jwt2": "^7.1.3",
    "hapi-boom-decorators": "^2.0.2",
    "hapi-router": "^3.5.0",
    "inert": "^4.0.2",
    "install": "^0.8.1",
    "joi": "^9.0.4",
    "joi-objectid": "^2.0.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "mongorito": "github:vdemedes/mongorito",
    "nodemailer": "^2.5.0",
    "npm": "^3.10.8"
  }
}
